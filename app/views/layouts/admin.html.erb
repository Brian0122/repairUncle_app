<!DOCTYPE html>
<html>
<head>
  <title>RepairUncle</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<header class="navbar navbar-fixed-top">
      <nav class="navbar-inner">
        <div class="container">
          <%= render 'layouts/navigation' %>
        </div>
      </nav>
    </header>

<div id="main" role="main">
      <div class="container">
         <div class="row-fluid">
         
          <div class="span2">
          <div class="well sidebar-nav">
            <ul class="nav nav-list">
              <li class="nav-header">Functions</li>
              <h4><%= link_to 'User', admin_users_path %></h4>
              <h4><%= link_to 'Workshop', workshops_path %></h4>
              <h4><%= link_to 'Make', makes_path %></h4>
              <h4><%= link_to 'Model', models_path %></h4>
              <h4><%= link_to 'Repair', repairs_path %></h4>
            </ul>
          </div><!--/.well -->
        </div>
        <div class="content span10">
           
              <%= render 'layouts/messages' %>
              <%= yield %>
           
          <footer>
          </footer>
        </div>
        </div>
      </div> <!--! end of .container -->
    </div> <!--! end

</body>
</html>
